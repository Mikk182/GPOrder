@model IEnumerable<GPOrder.Models.OrderLine>

@Ajax.ActionLink("Add", "GetNewOrderLine", new AjaxOptions { InsertionMode = InsertionMode.InsertAfter, UpdateTargetId = "OrderLinesDiv", OnComplete = "_order.ApplyJQuery();_order.SubscribeDeleteLinkClick();" })
<div id="OrderLinesDiv">
    @if (Model != null)
    {
        foreach (var orderLine in Model)
        {
            @Html.EditorFor(m => orderLine)
        }
    }
</div>